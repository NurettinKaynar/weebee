{
  "expo": {
    "name": "Weebee",
    "slug": "weebee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "weebee",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nurettinkaynar.weebee"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#161622"
      },
      "package": "com.nurettinkaynar.weebee"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": "100%",
          "resizeMode": "contain",
          "backgroundColor": "#161622",
          "androidSplashResourceName": "splash",
          "androidSplashDrawableType": "XML"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
